<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MultiStateView">
        <attr name="msv_loadView" format="reference"></attr>
        <attr name="msv_emptyView" format="reference"></attr>
        <attr name="msv_errorView" format="reference"></attr>
        <attr name="viewState" format="enum">
            <enum name="content" value="0"></enum>
            <enum name="error" value="1"></enum>
            <enum name="empty" value="2"></enum>
            <enum name="loading" value="3"></enum>

        </attr>
    </declare-styleable>

</resources>